<?xml version="1.0" encoding="UTF-8"?>
<config>
<application
    id="stats"
    title="Affichage de statistiques avec un plugin Mviewer"
    logo=""
    help=""
    style=""
    exportpng="false"
    showhelp="false"
    coordinates="false"
    measuretools="false"
    togglealllayersfromtheme="false">
</application>
<mapoptions maxzoom="18" projection="EPSG:3857" center="-339635.95753680886, 6138515.503773717" zoom="9" />
<baselayers style="default">
    <baselayer visible="true" id="positron" thumbgallery="img/basemap/positron.png" title="CartoDb" label="Positron" type="OSM" url="https://{a-c}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png" attribution="Map tiles by  &lt;a href=&quot;https://cartodb.com/attributions&quot;&gt;CartoDb&lt;/a&gt;, under  &lt;a href=&quot;https://creativecommons.org/licenses/by/3.0/&quot;&gt;CC BY 3.0 &lt;/a&gt;"  ></baselayer>
        <baselayer  type="WMTS" id="ortho" label="Photo aérienne IGN" title="GéoPortail" maxscale="1000" thumbgallery="img/basemap/ortho.jpg"
            url="https://data.geopf.fr/wmts" layers="ORTHOIMAGERY.ORTHOPHOTOS" format="image/jpeg" visible="false" fromcapacity="false"
            attribution="&lt;a href='https://geoservices.ign.fr/services-geoplateforme-diffusion' target='_blank'>&lt;img src='img/basemap/geoservices.png'>&lt;/a>" style="normal" matrixset="PM" maxzoom="22"/>
</baselayers>
<extensions>
        <extension type="component" id="stats" path="demo/addons"/>
</extensions>
<themes mini="true" legendmini="true">
    <theme name="Structures"  collapsed="true" id="demo" icon="fas fa-laptop-house">
        <layer id="structures"
            toplayer="true"
            name="Sites pour l'aide économique et l'innovation"
            visible="true"
            url="demo/filter/customlayers/structures.js"
            queryable="true"
            type="customlayer"
            opacity="1"     
            expanded="true"
            attribution="Source : &lt;a href='https://data.bretagne.bzh' target='_blank'>Open Data Région Bretagne &lt;/a>"
            metadata=""
            metadata-csw="">
        </layer>
    </theme>
     <theme name="Population"  collapsed="true" id="population" icon="fas fa-laptop-house">
        <layer id="population_communes"
            toplayer="true"
            name="Population des communes"
            visible="true"
            url="demo/stats/customlayers/population_communes.js" 
            queryable="true"
            type="customlayer"
            opacity="1"     
            expanded="true"
            attribution="Source : OSM"
            metadata=""
            metadata-csw="">
        </layer>
    </theme>
    <theme name="Limites" collapsed="true" id="limites">
        <layer 
            id="commune" 
            name="Commune"
            visible="true"
            queryable="false" 
            fields="nom_geo"
            aliases="Nom" 
            type="customlayer"
            style=""
            opacity="1"
            legendurl="img/legend/commune.png" 
            url="demo/filter/customlayers/communes.js" 
            tooltip="true"
            attribution="Source: GéoBretagne" />
    </theme>
</themes>
</config>